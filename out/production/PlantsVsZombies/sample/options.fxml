<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane fx:id="optionPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Options">
   <children>
      <Button fx:id="saveGame" layoutX="253.0" layoutY="91.0" mnemonicParsing="false" text="Save Game" />
      <Button fx:id="quit" layoutX="273.0" layoutY="208.0" mnemonicParsing="false" onMouseClicked="#clickExit" text="Quit" />
      <Button fx:id="load" layoutX="253.0" layoutY="150.0" mnemonicParsing="false" text="Load Game" />
      <Button fx:id="back" layoutX="471.0" layoutY="28.0" mnemonicParsing="false" onMouseClicked="#backToGame" text="Back to Game" />
   </children>
</AnchorPane>
